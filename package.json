{
  "name": "Dusken",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "emulator": "emulator -avd $(emulator -list-avds | head)&",
    "build-android": "cd android && ./gradlew assembleRelease",
    "release-android": "cd android && bundle exec fastlane beta",
    "release-ios": "cd ios && bundle exec fastlane beta"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.9",
    "apollo-client": "^2.4.1",
    "apollo-link-http": "^1.5.4",
    "email-validator": "^2.0.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "moment": "^2.22.2",
    "native-base": "^2.8.0",
    "prop-types": "^15.6.2",
    "react": "16.4.1",
    "react-apollo": "^2.1.11",
    "react-native": "0.56.0",
    "react-native-confetti": "^0.0.6",
    "react-native-config": "^0.11.5",
    "react-native-htmlview": "^0.13.0",
    "react-native-sentry": "^0.39.0",
    "react-native-vector-icons": "~4.4.2",
    "react-navigation": "^2.12.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.10.2",
    "redux-thunk": "^2.3.0",
    "tipsi-stripe": "^4.5.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "jest": "23.5.0",
    "jest-fetch-mock": "^1.6.5",
    "react-dom": "^16.4.2",
    "react-test-renderer": "16.4.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|react-navigation|native-base|@shoutem|tipsi-stripe)"
    ]
  }
}
